<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="HAHA" package="com.davidevans.games.haha" version="1.0.0" company="Cortex Semi" />
	<app main="Main" path="Export" file="HAHA" />
	<!-- -->
	<window width="480" height="320" orientation="landscape" background="0xffffff" />
	<window resizable="false" unless="mobile || console" />
	<window depth-buffer="true" stencil-buffer="true" />
	<!-- -->	
	<source path="Source" />
	<!-- -->
	<haxeflag name="-D source-map" />
	<!-- -->	
	<haxelib name="openfl" />
	<haxelib name="haxeui-core" />
	<haxelib name="spine-haxe" />	
	<haxelib name="actuate" />
	<haxelib name="starling" />
	<haxelib name="haxeui-openfl" /.
	<!-- -->
	<assets path="assets" include="*.skel" rename="assets" type="binary" />
	<assets path="assets/audio/wing_flap.mp3" if="web || air" embed="false" />
	<assets path="assets/audio/wing_flap.ogg" unless="flash" embed="false" />
	<assets path="assets/textures/1x" embed="false" />
	<assets path="assets/fonts/1x" embed="false" />
	<assets path="assets/fonts/DejaVuSans.ttf" />
	<assets path="assets/fonts/Ubuntu-R.ttf" />
</project>


